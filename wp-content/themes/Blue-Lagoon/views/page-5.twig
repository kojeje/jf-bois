{%  block content %}

<article class="article m-t0">
    {#Section accueil#}
    <section class="section w-80 w-md-90 m-a" id="section1">
        {#ancre 1#}
        <a id="article-anch-1" class="d-b" ></a>

        {#titre#}
        <h1 class="t4 p10 w-50 m-a t-a-center border-b-3px brd-b-color-2 border-l-3px brd-l-color-2">
            <i class="fal fa-tree-palm"></i>
            <span class="color-2">{{ post.title }}</span>
        </h1>

        {#thumbnail#}


            <img class="affiche w-50 w-md-30 m-a m-r50 d-b" src="{{ post.thumbnail.src }}" alt="">


        {#bloc texte#}

        <div class=" m-a m-b50">


            {#texte#}
            <div class="w-90 t1 t-md-3 p30">{{ post.get_field('field_5d31da555fcd3') }}</div>



        </div>
        {#bouton#}
        <div class="m-a w-100">
            {% include '/views/partials/buttons/anch-button/button-anch-2.twig' %}
        </div>
        
    </section>


    <section class="section w-100 w-md-90 m-a">
        {#ancre#}
        <a id="article-anch-2" class="d-b" ></a>
        <div class="m-b50">
            {#titre#}
            <h1 class="t4 p10 w-50 m-a t-a-center border-b-3px brd-b-color-2 border-l-3px brd-l-color-2">
                <i class="fal fa-tree-alt"></i>
                <span class="color-2">{{ post.get_field('field_5d31c860c6b0e') }}</span>

            </h1>
            {#texte#}
            <div class="w-md-90 w-md-70 t1 t-md-3 p30 m-a">
                <img class="affiche2 w-50 w-md-30 m-a m-l50 m-b30 d-b" src="{{ post.get_field('field_5d31c8e1c666f')}}" alt="">
                {{ post.get_field('field_5d31d57f6af97') }}
                {#image (md)#}

            </div>


        </div>



        <div class="m-a">
            {% include '/views/partials/buttons/anch-button/button-anch-3.twig' %}
        </div>
        <hr class="w-80 m-a bk-color-2 m-t100 d-none d-md-block">
    </section>

    {# Section prestas#}
    <section class="section w-100 w-md-90 m-a">
        <a id="article-anch-3" class="d-b" ></a>
        <h1 class="t6 p10 t-a-center border-b-3px brd-b-color-2 border-l-3px brd-l-color-2 w-md-30 w-90 m-a">
            <i class="fal fa-tree-large"></i>
            <span class="color-2">prestations</span>
        </h1>

        {% include '/views/partials/loop/function-loop.twig' %}

        <hr class="w-80 m-a bk-color-2 m-t100 d-b">
    </section>
    <section class="section url-1 w-md-80 w-100 m-a parallax2 contact_form rad10 rad-md-0">
        <a id="article-anch-4" class="d-b" ></a>
        <div class="row m-t10 bk-color-6 m-b20 w-90  m-a p10 rad10 rad-md-0">
            {% for contact in contacts %}
                {% if contact.terms('category')|join == 'contact' %}
                        <div class="offset-md-4 d-none d-md-block"></div>
                        <h1 class="col-12 col-md-4 t6 p10 t-a-center border-b-3px brd-b-color-2 border-l-3px brd-l-color-2 w-30 m-a">
                            <i class="fal fa-id-card-alt"></i>
                            {{ contact.title }}</h1>
                        <div class="offset-md-4 d-none d-md-block"></div>
                        <div class="col-12 col-md-6 order-md-1 order-2 relative">
                            <h1 class="m-t30 color-2">Envoyer un message</h1>
                            <div class="w-90 m-a">{{ contact.content}}</div>

                        </div>
                        <div class="col-12 col-md-6 p0 order-md-2 order-1">
                            <img class="img d-b m-a w-50" src="{{ theme.link }}\assets/img/logo.png" alt="">
                            <p class="t2 t-md-3 w-md-100 w-50 m-a m-t50 m-l30">7 Penot</p>
                            <p class="t2 t-md-3 w-md-100 w-50 m-a m-l30">33490 Pian sur Garonne</p>
                            <p class="t2 t-md-3 w-md-100 w-50 m-a m-l30"><i class="fal fa-phone-volume"></i> 06 61 23 14 77</p>
                            <iframe class="d-b w-100 w-md-80 m-md-a m-t30"src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2841.0327149826203!2d-0.2210206850278095!3d44.596356298949566!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xd556e84b997a949%3A0xbdc52a5a3cc5b6d6!2s7+Penot%2C+33490+Le+Pian-sur-Garonne!5e0!3m2!1sfr!2sfr!4v1563583034928!5m2!1sfr!2sfr" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                        </div>


                {% endif %}
            {% endfor %}
        </div>

    </section>


</article>
{% endblock %}