            </div>

            <div class="carousel-inner">

                <div class="carousel-item active">

                    <img class="d-block w-100" src="{{ post.get_field('field_5d319a58b7dbf') }}" alt="First slide">

                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="{{ post.get_field('field_5d319c80b7dc0') }}" alt="Second slide">
