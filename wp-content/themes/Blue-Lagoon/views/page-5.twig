                    <img class="d-block w-100" src="{{ post.get_field('field_5d319cb1b7dc2') }}" alt="Third slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="{{ post.get_field('field_5d319cddb7dc3') }}" alt="Fourth slide">
                </div>
                Si img4 existe
                {% if post.get_field('field_5d319cddb7dc3') %}
                    <div class="carousel-item">
                        <img class="d-block w-100" src="{{ post.get_field('field_5d319d03b7dc4') }}" alt="Fifth slide">
                    </div>
                {% endif %}
                Si img5 existe
                {% if post.get_field('field_5d319d03b7dc4') %}
                    <div class="carousel-item">
                        <img class="d-block w-100" src="{{ post.get_field('field_5d319d33b7dc5') }}" alt="sixth slide">
                    </div>
                {% endif %}
                Si img6 existe
                {% if post.get_field('field_5d319d33b7dc5') %}
                    <div class="carousel-item">
                        <img class="d-block w-100" src="{{ post.get_field('field_5d319d88b7dc6') }}" alt="seventh slide">
                    </div>
                {% endif %}
                Si img7 existe
                {% if post.get_field('field_5d319d88b7dc6') %}
                    <div class="carousel-item">
                        <img class="d-block w-100" src="{{ post.get_field('field_5d319da5b7dc7') }}" alt="eighth slide">
                    </div>
                {% endif %}
                Si img8 existe
                {% if post.get_field('field_5d319da5b7dc7') %}
                    <div class="carousel-item">
                        <img class="d-block w-100" src="{{ post.get_field('field_5d319dbdb7dc8') }}" alt="ninth slide">
                    </div>
                {% endif %}
                Si img9 existe
                {% if post.get_field('field_5d319dbdb7dc8') %}
